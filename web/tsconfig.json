{
  "compilerOptions": {
    "target": "ES6",
    "module": "NodeNext",
    "jsx": "react-jsx",
    "moduleResolution": "nodenext",
    "types": ["vite/client"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*"
  ]
}